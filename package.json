{
  "name": "giu",
  "version": "0.7.1",
  "description": "A collection of React components and utilities",
  "main": "lib/index.js",
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs",
    "ssr",
    "i18n"
  ],
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  },
  "scripts": {
    "start": "babel-node examples/server",
    "compile": "node package && rm -rf ./lib ./libEs6 && babel -d lib src && cp src/*.css lib && cp src/components/*.css lib/components && cp src/inputs/*.css lib/inputs && cp -r src/fonts lib/fonts && cp -r src libEs6",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "build": "npm run lint && npm run compile && npm run docs && npm run buildExamplesSsr && npm run xxl",
    "travis": "npm run compile",
    "buildExamples": "npm run buildExamplesSsr",
    "buildExamplesDev": "npm run buildExamplesCopy && cp examples/*.html docs/ && webpack --config examples/webpackConfig.js --progress --display-chunks --watch",
    "buildExamplesSsr": "npm run buildExamplesCopy && cross-env NODE_ENV=production SERVER_SIDE_RENDERING=true webpack --config examples/webpackConfig.js --progress --display-chunks -p",
    "buildExamplesSsrDev": "npm run buildExamplesCopy && cross-env SERVER_SIDE_RENDERING=true webpack --config examples/webpackConfig.js --progress --display-chunks --watch",
    "buildExamplesCopy": "cp -r examples/stylesheets docs/ && cp examples/favicon.ico docs/",
    "lint": "eslint src",
    "xxl": "xxl --src \"[\\\"src\\\"]\"",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "ava": "ava --watch",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testProd": "cross-env NODE_ENV=production nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-addons-pure-render-mixin": "^0.14.0 || ^15.0.0",
    "react-addons-shallow-compare": "^0.14.0 || ^15.0.0",
    "moment": "^2.0.0"
  },
  "dependencies": {
    "filesize": "3.3.0",
    "font-awesome": "4.6.3",
    "keycode": "2.1.1",
    "lodash": "4.15.0",
    "react-sortable-hoc": "0.0.7",
    "react-virtualized": "7.22.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "timm": "1.0.0",
    "tinycolor2": "1.4.1"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-docs": "^1.2.0",
    "faker": "3.0.1",
    "file-loader": "0.8.5",
    "fontfaceobserver": "1.7.1",
    "highlight.js": "9.5.0",
    "marked": "0.3.5",
    "moment": "^2.0.0",
    "nyc": "^6.1.1",
    "react": "^15.0.0",
    "react-addons-perf": "^15.0.0",
    "react-addons-pure-render-mixin": "^15.0.0",
    "react-dom": "^15.0.0",
    "static-site-generator-webpack-plugin": "2.1.0",
    "storyboard": "2.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "xxl": "^0.1.0"
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "babel": "inherit"
  }
}
