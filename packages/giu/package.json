{
  "name": "giu",
  "version": "0.16.0-alpha.0",
  "main": "lib/index.js",
  "scripts": {
    "compile": "yarn compile:clean && yarn compile:copy && yarn compile:run",
    "compileWatch": "yarn compile:clean && yarn compile:copy && yarn compile:runWatch",
    "compile:clean": "rm -rf ./lib",
    "compile:copy": "../../node_modules/.bin/flow-copy-source -i \"**/__mocks__/**\" -i \"**/__tests__/**\" src lib",
    "compile:run": "babel src -d lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\"",
    "compile:runWatch": "yarn compile:run --watch"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "moment": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {
    "filesize": "3.5.10",
    "keycode": "2.1.9",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react-sortable-hoc": "0.6.5",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "styled-jsx": "^2.2.6",
    "timm": "^1.2.5",
    "tinycolor2": "1.4.1",
    "unorm": "1.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.25.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "prettier": "^1.11.1"
  },
  "description": "A collection of React components and utilities",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs",
    "ssr",
    "i18n"
  ],
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  }
}
