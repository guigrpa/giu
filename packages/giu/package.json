{
  "name": "giu",
  "version": "0.17.0",
  "main": "lib/index.js",
  "scripts": {
    "compile": "yarn compile:clean && yarn compile:copy && yarn compile:css && yarn compile:run",
    "compileWatch": "yarn compile --watch",
    "compile:clean": "rm -rf ./lib",
    "compile:copy": "../../node_modules/.bin/flow-copy-source -i \"**/__mocks__/**\" -i \"**/__tests__/**\" src lib",
    "compile:css": "cp -r src/css lib/",
    "compile:run": "babel src -d lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\"",
    "compile:runWatch": "yarn compile:run --watch"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "filesize": "^3.6.1",
    "keycode": "^2.2.0",
    "lodash": "^4.17.10",
    "react-sortable-hoc": "^0.8.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "timm": "^1.6.1",
    "tinycolor2": "1.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "prettier": "^1.14.2"
  },
  "description": "A collection of React components and utilities",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs",
    "ssr",
    "i18n"
  ],
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  }
}
