{
  "name": "giu",
  "version": "0.11.1",
  "main": "lib/index.js",
  "scripts": {
    "compile": "yarn compile:clean && yarn compile:copy && yarn compile:run",
    "compileWatch": "yarn compile:clean && yarn compile:copy && yarn compile:runWatch",
    "compile:clean": "rm -rf ./lib",
    "compile:copy": "yarn compile:copy:flow && yarn compile:copy:css",
    "compile:copy:flow": "../../node_modules/.bin/flow-copy-source -i \"**/__mocks__/**\" -i \"**/__tests__/**\" src lib",
    "compile:copy:css": "cp src/*.css lib && cp src/components/*.css lib/components && cp src/inputs/*.css lib/inputs && cp src/vendor/*.css lib/vendor",
    "compile:run": "../../node_modules/.bin/babel src -d lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\"",
    "compile:runWatch": "yarn compile:run -- --watch"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "moment": "^2.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "dependencies": {
    "filesize": "3.5.9",
    "font-awesome": "4.7.0",
    "keycode": "2.1.1",
    "lodash": "4.17.4",
    "react-sortable-hoc": "0.0.8",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "timm": "^1.2.5",
    "tinycolor2": "1.4.1",
    "typeface-gloria-hallelujah": "0.0.22",
    "typeface-roboto": "0.0.22",
    "unorm": "1.4.1"
  },
  "description": "A collection of React components and utilities",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs",
    "ssr",
    "i18n"
  ],
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  }
}
