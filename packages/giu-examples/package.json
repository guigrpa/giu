    "moment": "^2.0.0",
    "next": "^4.1.4",
    "prettier": "^1.7.4",
    "prop-types": "15.5.10",
    "storyboard": "^3.0.0",
    "storyboard-preset-console": "^3.0.0",
    "styled-jsx": "^2.1.3",
