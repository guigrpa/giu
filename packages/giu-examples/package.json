  "version": "0.15.0-alpha.2",
  "scripts": {
    "compile": "echo Nothing",
    "compileWatch": "echo Nothing",
    "start": "next",
    "build": "next build && next export",
    "buildOld":
      "yarn build:copyPublic && yarn build:webpack -- -p --env.NODE_ENV=production",
    "buildWatch": "yarn build:copyPublic && yarn build:webpack -- -w",
    "buildSsr":
      "yarn build:copyPublic && yarn build:webpack -- -p --env.NODE_ENV=production --env.SERVER_SIDE_RENDERING",
    "buildSsrWatch":
      "yarn build:copyPublic && yarn build:webpack -- -w --env.SERVER_SIDE_RENDERING",
    "build:copyPublic": "cp -r src/public lib",
    "build:webpack":
      "../../node_modules/.bin/webpack --config ./webpackConfig.js --color --progress --display-chunks"
  },
